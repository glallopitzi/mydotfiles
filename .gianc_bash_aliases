## mine

export PATH="/Users/gianc/Dropbox/myScripts:$PATH"

## go to checkout folder
co() { cd ~/Documents/git/$1 && gst }
## go to backup folder
bck() { cd ~/Documents/git/bck/$1 && ls -ltrh }

drmstart() { docker-machine start default; }
drmrestart() { docker-machine restart default; }
drmstop() { docker-machine stop default; }
drmip() { docker-machine ip default; }

dreval() { eval $(docker-machine env default) }

drstop() { docker stop $(docker ps -a -q); }
drm() { docker rm $(docker ps -q -a); }
dri() { docker rmi $(docker images -q); }



